import time
from CASCCSD import CASCCSD

molecule mol {
    unit bohr
    0 1
    N 
    N 1 2.074
    symmetry c1
}

set {
    BASIS         cc-pvdz
    REFERENCE     RHF
    SCF_TYPE      PK
    SOSCF         True
    E_CONVERGENCE 12
    MAXITER       100
    MAX_ATTEMPTS  10
    GUESS         READ
}

N2 = CASCCSD(mol)

N2.compute('none', MP2_GUESS=True, E_CONV=12)

tpsi4 = time.time()
print('CCSD Energy:  {:<5.10f}'.format(energy('ccsd')))
print(time.time()-tpsi4)
