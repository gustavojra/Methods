import os
import sys
import numpy as np
from HTCCSD import HTCCSD

molecule mol {
    0 1
    H 
    Li 1 0.9
    symmetry c1
}

set {
    BASIS         sto-3g
    REFERENCE     RHF
    SCF_TYPE      PK
    SOSCF         True
    E_CONVERGENCE 12
    MAXITER       100
    MAX_ATTEMPTS  10
    GUESS         READ
    R_CONVERGENCE 10
}

X = HTCCSD(mol)

X.HTCCSD('full', CC_CONV=8, MP2_GUESS=False, RELAX_T3=True)

print('CAS energy:   {:<5.10f}'.format(X.Ecas))
print('CCSD Energy:  {:<5.10f}'.format(energy('ccsd')))
